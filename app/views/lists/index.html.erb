<% @lists.each do |list|%>
<div class="card" style="width: 18rem;">
  <%= image_tag "carr.jpg", alt: "Le Wagon", width: 200 %>
  <div class="card-body">
      <h5 class="card-title"><%=list.name%></h5>
  <%= link_to "show more", list_path(list), class: "btn btn-primary"%>
  </div>
    <% end %>
</div>
<%= link_to "create a list", new_list_path, class: "btn btn-primary"%>
